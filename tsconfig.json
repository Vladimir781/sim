{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "outDir": "game/dist",
    "rootDir": "game",
    "strict": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": false,
    "types": [],
    "lib": ["DOM", "ES2022", "WebWorker"],
    "skipLibCheck": true
  },
  "include": ["game/**/*.ts", "game/**/*.d.ts"],
  "exclude": ["game/dist"]
}
